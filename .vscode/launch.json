{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "bashdb",
            "request": "launch",
            "name": "Bash-Debug 3.gen",
            "program": "${workspaceFolder}/3.gen",
            "args": []
        },
        {

            "type": "java",
            "name": "Debug (Launch)-OpenAPIGenerator",
            "request": "launch",
            "mainClass": "org.openapitools.codegen.OpenAPIGenerator",
            "args": [
                "generate",
                "-i", "${workspaceFolder}/openapi.json.tmp",
                "-g", "go-oapi-codegen",
                "-o", "${workspaceFolder}/onshape",
                "--type-mappings", "DateTime=JSONTime",
                "--additional-properties", "packageVersion=${packageVersion}",
                "--additional-properties", "useOneOfDiscriminatorLookup=true",
                "--api-name-suffix", "Api",
                "--global-property", "apiTests=false",
                "-c", "${workspaceFolder}/openapi_config.json"
            ],
            // "projectName": "go-oapi-codegen",
            "classPaths": [
                "${workspaceFolder}/go-oapi-codegen.jar",
                "${workspaceFolder}/openapi-generator-cli.jar"
            ],
            "preLaunchTask": "combinedTask",
        }
    ]
}